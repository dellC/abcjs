{
  "name": "abcjs",
  "version": "3.1.1",
  "description": "Renderer for abc music notation",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "browserify index.js -o dist/abc.js --standalone ABCJS",
    "serve": "concurrently \"watchify index.js -o dist/abc.js --standalone ABCJS --debug -x canvas -x nw.gui\" \"live-server --watch=dist",
    "build:plugin": "browserify index-plugin.js -o dist/abc-plugin.js --standalone ABCJS",
    "serve:plugin": "concurrently \"watchify index-plugin.js -o dist/abc-plugin.js --standalone ABCJS\" \"live-server --watch=dist",
    "build:test": "browserify test/spec/index.js -o dist/abc-test.js",
    "serve:test": "concurrently \"watchify test/spec/index.js -o dist/abc-test.js --debug\" \"live-server --watch=dist --open=test\"",
    "test": "npm run build:test && mocha-phantomjs test/index.html",
    "build:coverage": "browserify -t browserify-istanbul test/spec/index.js -o dist/abc-test.js",
    "coverage": "npm run build:coverage && mocha-phantomjs test/index.html -k mocha-phantomjs-istanbul --reporter dot && istanbul report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dellC/abcjs.git"
  },
  "keywords": [
    "abc",
    "music",
    "notation"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dellC/abcjs/issues"
  },
  "homepage": "https://github.com/dellC/abcjs#readme",
  "devDependencies": {
    "browserify": "14.1.0",
    "browserify-istanbul": "2.0.0",
    "concurrently": "3.4.0",
    "coveralls": "2.13.0",
    "expect": "1.20.2",
    "expect.js": "0.3.1",
    "istanbul": "0.4.5",
    "live-server": "1.2.0",
    "mocha": "3.2.0",
    "mocha-phantomjs": "4.1.0",
    "mocha-phantomjs-istanbul": "0.0.2",
    "watchify": "3.9.0"
  },
  "dependencies": {
    "midi": "git://github.com/paulrosen/MIDI.js.git#abcjs",
    "raphael": "2.1.4"
  }
}
